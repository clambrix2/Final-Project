<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".History">

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="467dp"
        android:layout_height="165dp"
        android:layout_marginBottom="1dp"
        android:background="#D9BDA5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lv_v_history_historylist"
        app:srcCompat="@android:drawable/menuitem_background" />

    <ListView
        android:id="@+id/lv_v_history_historylist"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="133dp"
        android:layout_marginBottom="133dp"
        android:background="#D9BDA5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btn_v_history_search"
        android:layout_width="198dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lv_v_history_historylist"
        app:srcCompat="@drawable/search" />

    <ImageView
        android:id="@+id/btn_history_soical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:layout_marginEnd="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/posts_icon_new_new" />

    <ImageView
        android:id="@+id/btn_history_enemy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/btn_history_soical"
        app:layout_constraintTop_toTopOf="@+id/btn_history_soical"
        app:srcCompat="@drawable/enemies_icon_new_new" />

    <ImageView
        android:id="@+id/btn_history_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/btn_history_enemy"
        app:layout_constraintTop_toTopOf="@+id/btn_history_enemy"
        app:srcCompat="@drawable/items_icon_new_new" />

    <ImageView
        android:id="@+id/btn_history_weapon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toStartOf="@+id/btn_history_item"
        app:layout_constraintTop_toTopOf="@+id/btn_history_item"
        app:srcCompat="@drawable/weapons_icon_new_new" />

    <ImageView
        android:id="@+id/btn_history_user"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="3dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_history_weapon"
        app:layout_constraintTop_toTopOf="@+id/btn_history_weapon"
        app:srcCompat="@drawable/user_icon_new_new" />

</androidx.constraintlayout.widget.ConstraintLayout>