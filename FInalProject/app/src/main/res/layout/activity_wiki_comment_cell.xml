<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9BDA5"
    tools:context=".WikiComment_cell">

    <TextView
        android:id="@+id/tv_v_comments_cell_comment"
        android:layout_width="411dp"
        android:layout_height="91dp"
        android:singleLine="false"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btn_v_comments_cell_likes"
        android:layout_width="26dp"
        android:layout_height="20dp"
        android:layout_marginStart="252dp"
        android:layout_marginTop="68dp"
        android:src="@android:drawable/arrow_up_float"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btn_v_comments_cell_dislikes"
        android:layout_width="27dp"
        android:layout_height="17dp"
        android:layout_marginStart="316dp"
        android:layout_marginTop="68dp"
        android:src="@android:drawable/arrow_down_float"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_v_commentslist_likes"
        android:layout_width="37dp"
        android:layout_height="18dp"
        android:layout_marginStart="280dp"
        android:layout_marginTop="68dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_v_comments_cell_dislikes"
        android:layout_width="37dp"
        android:layout_height="19dp"
        android:layout_marginStart="340dp"
        android:layout_marginTop="68dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="416dp"
        android:layout_height="4dp"
        android:layout_marginTop="88dp"
        android:background="#000000"
        android:src="@android:drawable/menuitem_background"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>